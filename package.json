{
  "name": "aa-digital-teleport",
  "version": "1.1.0",
  "repository": "",
  "author": "Howard Chang <hschang.2008@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:client": "cd packages/client && yarn dev",
    "dev:server": "cd packages/server && yarn dev",
    "build": "run-s build:*",
    "build:server": "cd packages/server && yarn build",
    "build:client": "cd packages/client && yarn generate",
    "migrate:latest": "cd packages/server && yarn migrate:latest",
    "lint": "run-p lint:*",
    "lint:client": "cd packages/client && yarn lint",
    "lint:server": "cd packages/server && yarn lint"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
